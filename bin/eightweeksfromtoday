#!/usr/bin/python
# returns the date 8 weeks from today
import datetime
import calendar

today = datetime.date.today()
Weekday = ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']

eightweeks = datetime.timedelta(days=56)
day=today+eightweeks
wday=Weekday[datetime.date.weekday(day)]
print (wday+","+str(day))










